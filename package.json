{
  "name": "constellates",
  "version": "0.0.0",
  "description": "my React.js boilerplate",
  "scripts": {
    "watch": "watchify -d app/browser.js -o dist/bundle.js -v",
    "build-js": "browserify -d app/browser.js > dist/bundle.js",
    "build-css": "node-sass app/scss/app.scss dist/app.css --output-style compressed",
    "watch-css": "nodemon -e scss -x \"npm run build-css\"",
    "start": "babel-node app/server",
    "test": "jest",
    "lint": "eslint app",
    "doc": "esdoc -c esdoc.json",
    "postinstall": "npm run build-js && npm run doc"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/constellates/constellates.git"
  },
  "keywords": [
    "react"
  ],
  "author": "constellates",
  "license": "GPLv2",
  "bugs": {
    "url": "https://github.com/constellates/constellates/issues"
  },
  "devDependencies": {
    "babel-eslint": "^3.1.18",
    "babel-jest": "^4.0.0",
    "esdoc": "^0.1.2",
    "eslint": "^0.23.0",
    "eslint-plugin-react": "^2.5.2",
    "jest-cli": "0.4.0",
    "node-sass": "^3.2.0",
    "nodemon": "^1.3.8",
    "watchify": "^3.2.2"
  },
  "dependencies": {
    "babel": "^5.5.6",
    "babelify": "^6.1.2",
    "browserify": "^10.2.4",
    "classnames": "^2.1.2",
    "es6-react-mixins": "^0.1.8",
    "express": "^4.12.4",
    "flux": "^2.0.3",
    "object-assign": "^3.0.0",
    "react": "^0.13.3",
    "react-router": "^0.13.3",
    "superagent": "^1.2.0"
  },
  "browserify": {
    "transform": [
      [
        "babelify"
      ]
    ]
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "unmockedModulePathPatterns": [
      "node_modules/react"
    ]
  },
  "engines": {
    "node": "0.12.0"
  }
}
